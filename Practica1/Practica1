// A)

let nombre = "Armando"; // usamos let porque luego se reasigna
const edad = 25;        // usamos const porque la edad no cambia

nombre = "Ana Maria"; // reasignación permitida con let

const saludo = "Hola," + nombre + ".Tienes" + edad + "años.";

console.log(saludo); // muestra en consola el contenido de saludo



